      label="%featureName"
      version="4.3.1.qualifier"
      provider-name="%providerName">

   <description url="%optionalURL">
      %description
   </description>

   <copyright url="%copyrightURL">
      %copyright
   </copyright>

   <license url="%licenseURL">
      %license
   </license>

   <requires>
      <import plugin="org.eclipse.papyrus.diagram.common"/>
      <import plugin="org.eclipse.papyrus.tabbedproperties.core"/>
      <import plugin="org.topcased.requirement.core"/>
      <import feature="org.topcased.requirement.core.feature" version="4.3.1" match="greaterOrEqual"/>
      <import feature="org.eclipse.papyrus" version="0.8.2" match="greaterOrEqual"/>
   </requires>

   <plugin
         id="org.topcased.requirement.bundle.papyrus"
         download-size="0"
         install-size="0"
         version="0.0.0"
         unpack="false"/>

